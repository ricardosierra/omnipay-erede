{
    "name": "ricardosierra/omnipay-erede",
    "type": "library",
    "description": "Erede is brazilian acquirer solution that accept payments with MasterCard, Visa and Diners Club International credit cards on the Internet",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "erede",
        "purchase"
    ],
    "license": "MIT",
    "homepage": "https://github.com/ricardosierra/omnipay-erede",
    "authors": [
        {
            "name": "Ricardo Rebello Sierra",
            "email": "ricardo@sierratecnologia.com.br"
        },
        {
            "name": "Lucas Motizuki",
            "email": "motizuki@getbilo.com"
        }
    ],
    "autoload": {
        "psr-0": { "Omnipay\\Erede\\" : "src/" }
    },
    "require": {
        "php": ">=7.0.0",
        "omnipay/common": "^3"
    },
    "require-dev": {
        "omnipay/tests": "^3",
        "squizlabs/php_codesniffer": "^3",
        "phpro/grumphp": "^0.14"
    },
    "prefer-stable": true
}
